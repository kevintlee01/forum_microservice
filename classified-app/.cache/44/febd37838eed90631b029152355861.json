LogoutLink onClick={evt => {\n                    evt.preventDefault();\n                    dispatch(clearSession());\n                    deleteUserSession({variables: {sessionId: session.id}});\n                }}>(Logout)</LogoutLink>\n            </Wrapper>\n    );\n};\n\nexport default Account;"},"lineCount":null}},"error":null,"hash":"178b37a70fdb928bcdcc9550fcb6d532","cacheData":{"env":{}}}